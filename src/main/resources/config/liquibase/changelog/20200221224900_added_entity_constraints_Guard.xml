        <addForeignKeyConstraint baseColumnNames="doctor_medical_service_id"
                                 baseTableName="guard"
                                 constraintName="fk_guard_doctor_medical_service_id"
                                 referencedColumnNames="id"
                                 referencedTableName="medical_service"/>

        <addForeignKeyConstraint baseColumnNames="doctor_id"
                                 baseTableName="guard"
                                 constraintName="fk_guard_doctor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="doctor"/>

    </changeSet>
</databaseChangeLog>
